{
  "name": "executive-assistant-backend",
  "version": "1.0.0",
  "description": "Executive Assistant MVP Backend with AG-UI Protocol",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc && tscpaths -p tsconfig.json -s ./src -o ./dist",
    "start": "node dist/index.js",
    "test": "jest",
    "lint": "eslint src --ext .ts,.tsx",
    "format": "prettier --write src/**/*.{ts,tsx}"
  },
  "dependencies": {
    "@types/node": "^20.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0",
    "express": "^4.18.0",
    "ws": "^8.16.0",
    "uuid": "^9.0.0",
    "dotenv": "^16.0.0",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "rate-limiter-flexible": "^3.0.0",
    "google-auth-library": "^9.15.0",
    "googleapis": "^126.0.0",
    "openai": "^4.20.0",
    "langchain": "^0.2.0",
    "@langchain/openai": "^0.2.0",
    "@langchain/core": "^0.2.0",
    "zod": "^3.22.0",
    "jsonschema": "^1.4.1",
    "node-cron": "^3.0.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.0",
    "@types/ws": "^8.5.0",
    "@types/uuid": "^9.0.0",
    "@types/cors": "^2.8.0",
    "@types/node-cron": "^3.0.0",
    "jest": "^29.0.0",
    "@types/jest": "^29.0.0",
    "eslint": "^8.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "prettier": "^3.0.0",
    "tscpaths": "^0.0.9"
  },
  "keywords": [
    "executive-assistant",
    "ai-agent",
    "ag-ui",
    "langgraph",
    "typescript"
  ],
  "author": "Executive Assistant Team",
  "license": "MIT"
}
